CREATE TABLE v0 ( c1 INTEGER PRIMARY KEY, c2 INTEGER REFERENCES t1 ON DELETE CASCADE REFERENCES t2, c3 TEXT DEFAULT x'ABCD', FOREIGN KEY ( c1, c1 ) REFERENCES t2 ( c2, c3 ) ON UPDATE CASCADE ); 
CREATE TABLE v4 ( c5 CHECK( 100 ), c6 TEXT ); 
CREATE TABLE v7 ( c8 INTEGER REFERENCES t2, c9 INTEGER REFERENCES t1, FOREIGN KEY ( c9, c9 ) REFERENCES t2 ( c8, c9 ) ); 
INSERT INTO v0 VALUES ( 100, 100, 'x' ); 
INSERT INTO v4 VALUES ( 100, 100, 'x' ); 
INSERT INTO v0 VALUES ( 100, 'v1' ); 
INSERT INTO v0 VALUES ( 8, 'v1' ); 
INSERT INTO v4 VALUES ( 100, 100 ); 
INSERT INTO v4 VALUES ( 8, 0 ); 
SELECT MAX ( a10.c1 ) FROM v0 AS a10 ORDER BY 0; 
SELECT a11.c5 FROM v4 AS a11 ORDER BY 255; 
SELECT MAX ( a12.c8 ) FROM v7 AS a12 ORDER BY 8; 
SELECT a13.c8 FROM v7 AS a13 ORDER BY 1; 
SELECT MIN ( a14.c3 ) FROM v0 AS a14 ORDER BY 8; 
SELECT a15.c2 FROM v0 AS a15 ORDER BY 100; 
SELECT MAX ( a16.c9 ) FROM v7 AS a16 ORDER BY 8; 
SELECT ( a18.c1 ) FROM v0 AS a18 ORDER BY 1207000000; 
SELECT MIN ( a19.c8 ) FROM v7 AS a19, v7 AS a20 ORDER BY 1207000000; 
