CREATE TABLE v0 ( c1, c2, c3 REFERENCES p1 ( c1 ) ); 
CREATE TABLE v4 ( c5, c6, c7, c8 REFERENCES p1 ( c6 ) ); 
ALTER TABLE v0 DROP COLUMN c1; 
ALTER TABLE v4 DROP COLUMN c6; 
SELECT COUNT ( * ) FROM v0 AS a9, v0 AS a10 WHERE a10.c2 ORDER BY 0; 
SELECT COUNT ( * ) FROM v4 AS a11 WHERE a11.c8 ORDER BY 0; 
SELECT COUNT ( * ) FROM v4 AS a12 WHERE a12.c7 ORDER BY 0, 0; 
SELECT COUNT ( * ) FROM v0 AS a13 WHERE ( a13.c1 || a13.c3 || a13.c3 ) != 'one-c' ORDER BY 9223372036854775807; 
SELECT COUNT ( * ) FROM v0 AS a14, v0 AS a15 WHERE a14.c2 ORDER BY 9223372036854775807; 
SELECT COUNT ( * ) FROM v4 AS a16 WHERE a16.c7 ORDER BY 18446744073709551615 COLLATE hex; 
SELECT COUNT ( * ) FROM v4 AS a17 WHERE a17.c6 ORDER BY 0; 
SELECT COUNT ( * ) FROM v0 AS a18 WHERE a18.c1 ORDER BY 9223372036854775807; 
SELECT COUNT ( * ) FROM v0 AS a19 WHERE a19.c3 ORDER BY 0; 
SELECT COUNT ( * ) FROM v0 AS a20 WHERE a20.c3 ORDER BY 1; 
