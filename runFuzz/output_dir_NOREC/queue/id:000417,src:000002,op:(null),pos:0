CREATE TABLE v0 ( c1 INT, c2 ); 
INSERT INTO v0 VALUES ( 100, 'aaabbb' ); 
CREATE TABLE v3 ( c4 TEXT, c5 ); 
INSERT INTO v0 VALUES ( 'v0', 'xyzzy' ); 
CREATE TABLE v6 ( c7 TEXT, c8 ); 
INSERT INTO v0 VALUES ( 100, '2-two' ); 
INSERT INTO v3 VALUES ( 'ddd', 'v1' ); 
CREATE VIEW v9 AS SELECT max ( c1 || c2 ) FROM v0 AS a10 ORDER BY 1; 
SELECT * FROM v9 AS a11 ORDER BY 100; 
CREATE TABLE v12 AS SELECT * FROM v9 AS a13 ORDER BY 0; 
PRAGMA automatic_index = 6; 
SELECT COUNT ( * ) FROM v12 AS a14 WHERE c4 ORDER BY 0 COLLATE hex; 
SELECT COUNT ( * ) FROM v9 AS a15 WHERE a15.y ORDER BY 18446744073709551488; 
SELECT COUNT ( * ) FROM v9 AS a16 WHERE a16.y ORDER BY 9223372036854775807; 
SELECT COUNT ( * ) FROM v9 AS a17 WHERE a17.y ORDER BY 100; 
SELECT COUNT ( * ) FROM v3 AS a18 WHERE a18.c5 ORDER BY 18446744073709551488 COLLATE hex; 
SELECT COUNT ( * ) FROM v9 AS a19 WHERE a19.y < a19.y ORDER BY 18446744073709551488; 
SELECT COUNT ( * ) FROM v3 AS a20 WHERE a20.c5 < a20.c4 ORDER BY 18446744073709551488; 
SELECT COUNT ( * ) FROM v12 AS a21 WHERE c2 ORDER BY c4 COLLATE NOCASE, y; 
SELECT COUNT ( * ) FROM v9 AS a22, v12 AS a23 WHERE c5 ORDER BY 100; 
SELECT COUNT ( * ) FROM v12 AS a24 WHERE c7 ORDER BY 100; 
