CREATE TABLE v0 ( c1 INT, c2 INT ); 
ALTER TABLE v0 ADD COLUMN c3 BLOB DEFAULT x'ABCD'; 
INSERT INTO v0 ( c1, c3 ) VALUES ( 100, NULL ), ( NULL, NULL ), ( 100, NULL ); 
CREATE UNIQUE INDEX i4 ON v0 ( abs ( c3 ) ); 
SELECT COUNT ( * ) FROM v0 AS a5 WHERE a5.c2 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a6 WHERE a6.c2 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a7, v0 AS a8 NATURAL JOIN v0 AS a9 WHERE a9.c1 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a10, v0 AS a11 WHERE a11.c3 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a12 WHERE a12.c2 ORDER BY 0; 
SELECT COUNT ( * ) FROM v0 AS a13 WHERE a13.c3 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a14 WHERE a14.c2 ORDER BY 0 - ( a14.c1 + a14.c2 ); 
SELECT COUNT ( * ) FROM v0 AS a15 WHERE ( CASE WHEN a15.c3 != 'v0' THEN 100 ELSE 100 END ) ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a16 WHERE a16.c3 IS a16.c2 ORDER BY 100; 
SELECT COUNT ( * ) FROM v0 AS a17 WHERE hex ( hex ( 18446744073709518848 ) ) ORDER BY 100; 
