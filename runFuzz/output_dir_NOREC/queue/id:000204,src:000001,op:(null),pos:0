CREATE TABLE v0 ( c1 INT PRIMARY KEY ); 
CREATE TABLE v2 ( c3 INT PRIMARY KEY, c4 REAL ); 
CREATE VIEW v5 AS SELECT c1 FROM v2 AS a6 LEFT JOIN v0 AS a7 ON c1 = c1 ORDER BY 2147483647; 
CREATE VIEW v8 AS SELECT c1, c1 FROM v0 AS a9 RIGHT JOIN v0 AS a10 ON c1 = c1 ORDER BY 2147483647; 
CREATE VIEW v11 AS SELECT id, id FROM v8 AS a12 LEFT JOIN v5 AS a13 ON id = id ORDER BY 2147483647; 
CREATE VIEW v14 AS SELECT c3, c3 FROM v2 AS a15 RIGHT JOIN v2 AS a16 ON c3 = c3 ORDER BY 2147483647; 
CREATE VIEW v17 AS SELECT id, id FROM v14 AS a18 RIGHT JOIN v8 AS a19 ON id = id ORDER BY 100; 
