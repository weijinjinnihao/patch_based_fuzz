CREATE TABLE v0 ( c1 TEXT COLLATE BINARY ); 
ALTER TABLE v0 ADD COLUMN c2 INTEGER COLLATE NOCASE; 
INSERT INTO v0 VALUES ( 0, 'jack' ); 
INSERT INTO v0 VALUES ( 0, '10000 500' ); 
CREATE TABLE v3 ( c4 INTEGER ); 
INSERT INTO v3 VALUES ( 2147483647 ); 
INSERT INTO v0 VALUES ( 2147483647 ); 
ALTER TABLE v3 ADD COLUMN c5 INTEGER DEFAULT 0; 
SELECT COUNT ( * ) FROM v0 AS a6 WHERE a6.c2 ORDER BY 0; 
SELECT COUNT ( * ) FROM v0 AS a7 WHERE a7.c2 ORDER BY 0; 
SELECT COUNT ( * ) FROM v3 AS a8 WHERE a8.c5 ORDER BY 0; 
SELECT COUNT ( * ) FROM v0 AS a9 WHERE a9.c1 ORDER BY 2147483647; 
SELECT COUNT ( * ) FROM v3 AS a10 WHERE a10.c5 ORDER BY a10.c5 COLLATE binary; 
SELECT COUNT ( * ) FROM v0 AS a11 WHERE a11.c1 ORDER BY 2147483647; 
SELECT COUNT ( * ) FROM v0 AS a12 WHERE a12.c2 NOT NULL ORDER BY 2147483647; 
SELECT COUNT ( * ) FROM v0 AS a13 WHERE ( ( a13.c1 + 10 ) % 0 ) == 2147483647 ORDER BY 2147483647; 
SELECT COUNT ( * ) FROM v3 AS a14, v0 AS a15 WHERE a14.c4 ORDER BY 4294967295; 
SELECT COUNT ( * ) FROM v0 AS a16 WHERE a16.c2 IS NOT NULL ORDER BY 2147483647; 
